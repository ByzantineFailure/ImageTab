{
  "name": "ImageTab",
  "author": "byzanitnefailure@gmail.com <Michael Eaton>",
  "version": "0.0.1",
  "description": "Provide a context menu item when right-clicking images to open it in a new tab",
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "16": "assets/icon-16.png",
    "44": "assets/icon-44.png",
    "48": "assets/icon-48.png",
    "50": "assets/icon-50.png",
    "128": "assets/icon-128.png",
    "150": "assets/icon-150.png",
    "176": "assets/icon-176.png"
  },
  "background": {
    "scripts": ["ImageTab.js"],
    "persistent": true
  }
}
